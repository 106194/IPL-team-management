<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: skyblue;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fahim.fahim22.Client">
   <children>
      <ImageView fitHeight="626.0" fitWidth="801.0" layoutX="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@search.jpg" />
         </image>
      </ImageView>
      <ButtonBar layoutX="41.0" layoutY="138.0" prefHeight="40.0" prefWidth="200.0" />
      <ButtonBar layoutX="126.0" layoutY="342.0" prefHeight="40.0" prefWidth="200.0" />
      <ButtonBar layoutX="41.0" layoutY="216.0" prefHeight="40.0" prefWidth="200.0" />
    <Button fx:id="submit" layoutX="127.0" layoutY="531.0" mnemonicParsing="false" onAction="#submit" style="-fx-background-color: green;" text="Submit" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="22.0" />
         </font>
      </Button>
      <PasswordField fx:id="password" layoutX="300.0" layoutY="452.0" prefHeight="37.0" prefWidth="200.0" promptText="Password">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </PasswordField>
    <Button fx:id="reset" layoutX="560.0" layoutY="531.0" mnemonicParsing="false" onAction="#Input" prefHeight="46.0" prefWidth="128.0" style="-fx-background-color: green;" text="Reset" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="22.0" />
         </font>
      </Button>
      <TextField fx:id="clubName" layoutX="300.0" layoutY="402.0" prefHeight="38.0" prefWidth="200.0" promptText="Club Name">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
