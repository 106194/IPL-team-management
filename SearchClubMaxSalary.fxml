<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fahim.fahim22.MainController">
   <children>
      <ImageView fitHeight="608.0" fitWidth="809.0" layoutY="-7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@search.jpg" />
         </image>
      </ImageView>
      <TextField layoutX="562.0" layoutY="135.0" prefHeight="40.0" prefWidth="0.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <ButtonBar layoutX="362.0" layoutY="334.0" prefHeight="40.0" prefWidth="200.0" />
      <ButtonBar layoutY="166.0" prefHeight="40.0" prefWidth="200.0" />
    <Button fx:id="bname" layoutX="671.0" layoutY="518.0" mnemonicParsing="false" onAction="#ClubName" prefHeight="40.0" prefWidth="98.0" style="-fx-background-color: black;" text="Back" textFill="RED">
         <font>
            <Font name="System Bold Italic" size="21.0" />
         </font>
      </Button>
    <Button fx:id="bback" layoutX="333.0" layoutY="277.0" mnemonicParsing="false" onAction="#MaxSalary" style="-fx-background-color: green;" text="Confirm" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Button>
      <TextField fx:id="Club" alignment="CENTER" layoutX="308.0" layoutY="208.0" prefHeight="40.0" prefWidth="164.0" promptText="Club Name" style="-fx-background-radius: 15;">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
